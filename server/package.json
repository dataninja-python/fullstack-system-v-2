    "dependencies": {
        "express": "^4.18.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.7",
        "@types/node": "^18.7.5",
        "@typescript-eslint/parser": "^5.33.1",
        "eslint": "^8.22.0",
        "nodemon": "^2.0.19",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
}