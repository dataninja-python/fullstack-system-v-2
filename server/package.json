{
    "name": "server",
    "version": "0.0.2",
    "description": "This application automates the forms needed to automate creating an invoice",
    "main": "server.js",
    "scripts": {
        "start": "node ./dist/server.js",
        "tsc": "tsc",
        "postinstall": "pnpm run tsc",
        "dev":  "ts-node-dev --respawn --pretty --tranpile-only server.ts",
